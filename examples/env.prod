# Production Environment Configuration (.env)
# Copy this file to .env in your project root for production use

# API Keys (replace with your actual production keys)
ALPACA_API_KEY=your_prod_alpaca_api_key_here
ALPACA_SECRET_KEY=your_prod_alpaca_secret_key_here
OPENAI_API_KEY=your_prod_openai_api_key_here

# Environment (explicitly set to live for production)
ENVIRONMENT=live

# API Endpoints for live trading (production)
ALPACA_API_URL=https://api.alpaca.markets
ALPACA_DATA_API_URL=https://data.alpaca.markets

# Trading Configuration (production settings)
TARGET_INDEX_FUNDS=SPY,QQQ,DIA 